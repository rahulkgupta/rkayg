{
  "metadata": {
    "title": "rkayg",
    "description": "I'm lost."
  },
  "plugins": {
    "metalsmith-collections": {
      "posts": {
        "pattern": "posts/*.md",
        "sortBy": "date",
        "reverse": true
      }
    },
    "metalsmith-permalinks": {
      "pattern": ":collection/:title",
      "relative": "off"
    },
    "metalsmith-markdown": {},
    "metalsmith-templates": {
      "engine": "handlebars",
      "directory":"templates",
      "partials": {
        "header": "partials/header",
        "footer": "partials/footer"
      }
    }
  }
}
